{
    "name": "mhor/php-mediainfo",
    "type": "library",
    "description": "PHP wrapper around the mediainfo command",
    "keywords": [
        "mediainfo",
        "wrapper",
        "metadata",
        "id3"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "mhor",
            "email": "maxime.horcholle@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/process": "~5.0|~6.0|~7.0|~8.0",
        "symfony/filesystem": "~5.0|~6.0|~7.0|~8.0"
      },
    "require-dev": {
        "phpunit/phpunit": "~8.5",
        "phpspec/prophecy": "^1.15"
    },
    "autoload": {
        "psr-4": {
            "Mhor\\MediaInfo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mhor\\MediaInfo\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        }
    }
}
