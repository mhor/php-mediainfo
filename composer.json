{
    "name": "mhor/php-mediainfo",
    "type": "library",
    "description": "PHP wrapper around the mediainfo command",
    "keywords": [
        "mediainfo",
        "wrapper",
        "metadata",
        "id3"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "mhor",
            "email": "maxime.horcholle@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/process": "~3.4|~4.0|~5.0",
        "symfony/filesystem": "~3.4|~4.0|~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.5"
    },
    "autoload": {
        "psr-4": {
            "Mhor\\MediaInfo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mhor\\MediaInfo\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        }
    }
}
