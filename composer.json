{
    "name": "mhor/php-mediainfo",
    "type": "library",
    "description": "PHP wrapper around the mediainfo command",
    "license": "MIT",
    "authors": [
        {
            "name": "mhor",
            "email": "maxime.horcholle@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/process": "^3.3|~4.0",
        "symfony/filesystem": "^3.3|~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Mhor\\MediaInfo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mhor\\MediaInfo\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev",
            "dev-fix-process-call": "4.0.x-dev"
        }
    }
}
